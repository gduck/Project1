<!DOCTYPE html>
<html>
  <head>
    <title>want MAW</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript" src="https://code.jquery.com/ui/1.11.2/jquery-ui.min.js"></script>

    <%= csrf_meta_tags %>
  </head>

  

  <body>
    <!-- main content put into a 10 wide column for white space either side -->
    <div class="container">
      <div class="row">
        <div class="col-md-10 col-md-offset-1 col-xs-12">
          
          <header id="header">    
            <%= render 'layouts/header' %>
          </header>

          <main id="main">

            <%= render 'layouts/logospace' %>
            <%= render 'layouts/linkbar' %>

            <%= yield %>
          </main>

          <footer class="footer">
            <div class="container-fluid">
              <div class="row">
                <p class="text-muted">footer here.</p>
              </div>
            </div>
          </footer>

        </div>
      </div>
    </div>
  </body>

</html>
